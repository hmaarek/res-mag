<p id="notice"><%= notice %></p>

<h1>Listing Term Devs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th>Rackid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @term_devs.each do |term_dev| %>
      <tr>
        <td><%= term_dev.name %></td>
        <td><%= term_dev.type %></td>
        <td><%= term_dev.RackID %></td>
        <td><%= link_to 'Show', term_dev %></td>
        <td><%= link_to 'Edit', edit_term_dev_path(term_dev) %></td>
        <td><%= link_to 'Destroy', term_dev, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Term dev', new_term_dev_path %>
